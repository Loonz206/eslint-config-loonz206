{
  "name": "eslint-config-loonz206",
  "version": "1.0.6",
  "description": "a config for loonz206",
  "main": "index.js",
  "keywords": [
    "javascript",
    "ecmascript",
    "eslint",
    "eslint-config",
    "lint",
    "config",
    "prettier"
  ],
  "scripts": {
    "commit": "npx git-cz",
    "format": "npm run pretty -- --write",
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx .",
    "pretty": "prettier --ignore-path .gitignore \"**/*.+(js|json|md)\"",
    "test": "echo 'Roads? Where we are going we dont need roads - Doc Brown'",
    "validate": "npm-run-all --parallel check-format lint",
    "prepare": "husky install",
    "publish": "npm publish"
  },
  "repository": "git@github.com:loonz206/eslint-config-loonz206",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Lenny Peters",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "dependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2"
  }
}
